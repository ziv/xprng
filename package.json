{
  "name": "xprng",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build-docs": "ng build  @apps/docs",
    "build": "./ng-build.sh",
    "test": "./ng-test.sh",
    "docs": "compodoc -p tsconfig.doc.json",
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "serve:ssr:@xpr/docs": "node dist/@xpr/docs/server/server.mjs",
    "serve:ssr:@xprng/docs": "node dist/@xprng/docs/server/server.mjs",
    "serve:ssr:@apps/docs": "node dist/@apps/docs/server/server.mjs",
    "serve:ssr:@apps/demo": "node dist/@apps/demo/server/server.mjs"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "dependencies": {
    "@angular/platform-server": "^20.1.0",
    "@angular/ssr": "^20.1.5",
    "express": "^5.1.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.5",
    "@angular/cdk": "^20.1.5",
    "@angular/cli": "^20.1.5",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/compiler-cli": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@compodoc/compodoc": "^1.1.26",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.17.19",
    "@types/prismjs": "^1.26.5",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.0.1",
    "dompurify": "^3.2.6",
    "highlight.js": "^11.11.1",
    "jasmine-core": "~5.8.0",
    "jsdom": "^26.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "marked": "^16.1.2",
    "marked-highlight": "^2.2.2",
    "ng-packagr": "^20.1.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "semantic-release": "^24.2.7",
    "shiki": "^3.9.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.2",
    "vitest": "^3.2.4"
  },
  "compodoc": {
    "name": "XPRNG",
    "output": "docs",
    "theme": "postmark",
    "hideGenerator": true,
    "disableSourceCode": true,
    "disableDomTree": true,
    "disableTemplateTab": true
  }
}
