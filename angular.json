    "@xprng/slides": {
      "projectType": "library",
      "root": "xprng/slides",
      "sourceRoot": "xprng/slides/src",
      "prefix": "xpr",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
